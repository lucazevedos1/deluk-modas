<script>
  async function loadScripts() {
    const jqueryPromise = new Promise((resolve, reject) => {
      const script = document.createElement('script');
      script.src = "{{ 'jquery-3-7.min.js' | asset_url }}";
      script.onload = resolve;
      script.onerror = reject;
      document.head.appendChild(script);
    });

    const slickPromise = new Promise((resolve, reject) => {
      const script = document.createElement('script');
      script.src = "{{ 'slick.min.js' | asset_url }}";
      script.onload = resolve;
      script.onerror = reject;
      document.head.appendChild(script);
    });

    await Promise.all([jqueryPromise, slickPromise]);
  }

  async function initializeSlider() {
    await loadScripts();

    $(document).ready(function () {
      $('.slide-div').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: true,
        fade: false,
        asNavFor: '.slider-nav',
        autoplay: false,
        autoplaySpeed: 11000,
        dots: false,
      });

      $('.slide-div').on('afterChange', function(event, slick, currentSlide, nextSlide){
        $('video.prd-slide').each(function() {
            this.pause();
            this.currentTime = 0;
          });
        $('.prd-slide__video-play').css('visibility', 'visible');
        $('.prd-slide__video-play').css('opacity', 1);
        $('.prd-slide__video').prop('muted', true);
        $('.prd-slide__video-play.mute span').text('Tap for sound');
        $('.prd-slide__video-wrapper').removeClass('active');
        let $video = $('.slick-current video.prd-slide');
        if ($video.length > 0) {
          $video.get(0).play();
        }
      });
      
      $('.slider-nav').slick({
        slidesToShow: 5,
        slidesToScroll: 1,
        asNavFor: '.slide-div',
        dots: false,
        centerMode: false,
        focusOnSelect: false,
        infinite: true,
        arrows: false,
        focusOnSelect: true,
        swipeToSlide: true,
        pauseOnHover:false,
        respondTo:'min',
        rtl: false,
        cssEase:'linear'
      });
    /* Video Play/Pause Events */
    $('.prd-slide__video').prop('muted', true);
    $('.prd-slide__video').on('click play pause', function (e) { 
      let $target = $(this);
      const $playBtn = $target.siblings('.prd-slide__video-play');
      const eventType = e.originalEvent.type;
      if(eventType !== 'click') {
        return $playBtn.css('display', 'flex');
      }
      $target = $target.get(0);
      soundMute($target);
    });
    $('.prd-slide__video-play').on('click', function (e) {
      const $video = $(this).siblings('.prd-slide__video').get(0);
      $(this).hide();
      soundMute($video);
    });

    function soundMute(target){
      if(!target.paused && $('.prd-slide__video').prop('muted') == false) {
        $('.prd-slide__video-play').css('visibility', 'visible');
        $('.prd-slide__video-play').css('opacity', 1);
        $('.prd-slide__video').prop('muted', true);
        $('.prd-slide__video-play.mute span').text('Tap for sound');
        $('.prd-slide__video').prop('volume', 0);
        $('.prd-slide__video-wrapper').removeClass('active');
         }
        else { 
          $('.prd-slide__video-play').css('visibility', 'hidden');
          $('.prd-slide__video-play').css('opacity', 0);
          $('.prd-slide__video').prop('muted', false);
          $('.prd-slide__video').prop('volume', 1);
          $('.prd-slide__video-play.play span').text('Tap for mute'); 
          $('.prd-slide__video-wrapper').addClass('active');
          target.play(); 
        }
    }
    });
  }

  initializeSlider();

  
</script>

