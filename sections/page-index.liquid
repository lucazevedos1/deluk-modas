<script>
  function redirectToCheckout() {
    window.location.href = '/checkout';
  }
  async function addItemToCart(id, quantity) {
    try {
      let response = await fetch(window.Shopify.routes.root + 'cart/add.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ items: [{ id: id, quantity: quantity }] })
      });

      if(response.status === 200) { 
        redirectToCheckout();
      }
    }
    catch(e) {
      console.log('[INDEX] ERROR ON ADDING ITEM TO CART AND/OR PROCEEDING TO CHECKOUT.');
      console.error(e);
    }
  }
  const btn = document.querySelector('.addTocut-btn');

      btn.addEventListener('click', (e) => { 
        const spinner = document.querySelector('.spinner__wrapper');
        spinner.classList.add('show');
        const productId = btn.getAttribute('data-product-id');
        const productQty = btn.getAttribute('data-product-qty');
        addItemToCart(
          productId,
          productQty
        ); 
      });
</script>

